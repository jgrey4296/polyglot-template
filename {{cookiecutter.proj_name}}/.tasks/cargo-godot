# #  cargo-godot -*- mode: sh -*-

set -euo pipefail

while [[ $# -gt 0 ]]; do
    case $1 in
        -t|--target)
            echo "Target: $2"
            ;;
        --*=*)
            echo "Assignment: $1"
            ;;
        *) # Positional
            echo "Positional: $1"
            ;;
    esac
    shift
done

